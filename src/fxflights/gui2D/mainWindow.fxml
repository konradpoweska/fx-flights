<HBox minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxflights.gui2D.Controller">
   <children>
      <SubScene fx:id="earth3D" height="600.0" width="600.0">
        <root>
          <Region />
        </root>
      </SubScene>
      <VBox prefWidth="260.0" spacing="12.0">
         <children>
            <GridPane hgap="6.0" vgap="6.0">
              <columnConstraints>
                <ColumnConstraints maxWidth="112.5" minWidth="10.0" prefWidth="45.0" />
                <ColumnConstraints maxWidth="189.0" minWidth="10.0" prefWidth="189.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" />
              </rowConstraints>
               <children>
                  <Label text="From" />
                  <Label text="To" GridPane.rowIndex="1" />
                  <ChoiceBox fx:id="fromChoiceBox" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" />
                  <ChoiceBox fx:id="toChoiceBox" maxWidth="1.7976931348623157E308" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <Button fx:id="searchButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Search" />
            <VBox spacing="6.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Flights" />
                  <ListView fx:id="flightsList" prefHeight="361.0" prefWidth="240.0" />
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</HBox>
